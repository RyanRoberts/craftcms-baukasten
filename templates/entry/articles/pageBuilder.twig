{% extends "_layouts/generic-page-layout.twig" %}
{% set bodyClass = 'p-pages p-pageBuilder' %}

{% block headLinks %}
  {{ parent() }}
{% endblock headLinks %}

{# Critical CSS #}
{% block inlineCriticalCss %}
  <style>
    {{ craft.twigpack.includeFile('@webroot/dist/criticalcss/' ~ _self ~  '_critical.min.css') }}
  </style>
{% endblock %}

{% block content %}
  {# Page Builder #}
  {% include '_builders/_pageBuilder.twig' with {
    props: {
      object: entry
    }
  } only %}
{% endblock %}

{% block subcontent %}{% endblock %}

{# Any JavaScript that should be included before </body> #}
{% block bodyJs %}
  {{ parent() }}
{% endblock bodyJs %}
