{
  "name": "buildchain",
  "description": "A drop-in buildchain for using Vite in your Craft CMS plugins or modules",
  "author": {
    "email": "david@hellmann.io",
    "name": "David Hellmann",
    "url": "https://davidhellmann.com"
  },
  "dependencies": {
    "@splidejs/splide": "^2.4.21",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "alpinejs": "^3.2.4",
    "axios": "^0.21.1",
    "bowser": "^2.11.0",
    "clipboard": "^2.0.8",
    "ferdi": "0.0.21",
    "lightgallery": "^2.1.8",
    "lodash-es": "^4.17.21",
    "tailwindcss-question-mark": "^0.1.1",
    "typeface-jetbrains-mono": "^2.0.0",
    "typeface-lato": "^1.1.13",
    "typeface-merriweather": "^1.1.13",
    "vue": "^3.0.5",
    "vue-axios": "^3.2.5",
    "vue-confetti": "^2.0.8"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@vitejs/plugin-legacy": "^1.3.3",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.2.5",
    "npm-check": "^5.9.2",
    "postcss": "^8.2.12",
    "postcss-nesting": "^8.0.1",
    "rollup-plugin-critical": "^1.0.4",
    "tailwind-config-viewer": "^1.6.2",
    "tailwindcss": "^2.1.2",
    "vite": "^2.3.2",
    "vite-plugin-favicon": "^1.0.8",
    "vite-plugin-restart": "0.0.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "upgrade-interactive": "npm-check --update",
    "preinstall": "$npm_execpath --silent run checknpm",
    "checknpm": "echo \"$npm_execpath\" | grep -q \"npm-cli\\.js$\" || (echo '⚠️ Use npm not yarn! No Hipster Party here!! ⚠️ ' && echo && exit 1)",
    "tcv": "tailwind-config-viewer -o",
    "tcc": "tailwind-config-viewer export ../_tailwind",
    "twc": "npx tailwindcss build ../src/css/app.pcss -o ../src/css/tailwind.app.css -c tailwind.config.js"
  },
  "version": "1.1.3"
}
