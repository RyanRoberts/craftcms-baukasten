{
  "require": {
    "barrelstrength/sprout-encode-email": "2.0.7",
    "cavellblood/craft-string-base64": "1.0.1",
    "craftcms/cms": "^3.7.0-beta.3",
    "craftcms/feed-me": "4.3.6",
    "craftcms/mailgun": "1.4.3",
    "craftcms/redactor": "2.8.7",
    "doublesecretagency/craft-cpcss": "2.3.0",
    "luwes/craft3-codemirror": "1.0.2",
    "marionnewlevant/twig-perversion": "2.2.0",
    "mmikkel/cp-clearcache": "1.1.0",
    "mmikkel/cp-field-inspect": "1.2.5",
    "nystudio107/craft-emptycoalesce": "^1.0.6",
    "nystudio107/craft-minify": "^1.2.5",
    "nystudio107/craft-retour": "3.1.54",
    "nystudio107/craft-scripts": "^1.2.4",
    "nystudio107/craft-seomatic": "3.3.43",
    "nystudio107/craft-twigpack": "1.2.14",
    "nystudio107/craft-twigprofiler": "^1.0.0",
    "nystudio107/craft-webperf": "1.0.24",
    "nystudio107/dotenvy": "^1.1.0",
    "ostark/craft-async-queue": "2.2.0",
    "putyourlightson/craft-blitz-recommendations": "1.2.0",
    "putyourlightson/craft-elements-panel": "1.1.1",
    "sebastianlenz/linkfield": "^2.0-beta",
    "spacecatninja/imager-x": "v3.4.0",
    "studioespresso/craft-dumper": "2.0.0",
    "verbb/formie": "1.4.7",
    "verbb/navigation": "1.4.18",
    "verbb/super-table": "2.6.8",
    "vlucas/phpdotenv": "^3.4.0",
    "yiisoft/yii2-redis": "^2.0.6",
    "yiisoft/yii2-shell": "^2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "modules\\sitemodule\\": "modules/sitemodule/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "craft-update": [
      "@pre-craft-update",
      "@post-craft-update"
    ],
    "create-folders": [
      "mkdir -p ./storage",
      "mkdir -p ./config/project",
      "mkdir -p ./web/cpresources",
      "mkdir -p ./web/assets"
    ],
    "pre-craft-update": [
      "@create-folders"
    ],
    "post-craft-update": [
      "@php craft install/check && php craft migrate/all --no-content || exit 0",
      "@php craft install/check && php craft project-config/apply --force || exit 0",
      "@php craft install/check && php craft migrate --track=content || exit 0",
      "@php craft install/check && php craft clear-caches/compiled-templates || exit 0",
      "@php craft install/check && php craft clear-caches/data || exit 0",
      "@php craft install/check && php craft clear-caches/temp-files || exit 0"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php craft setup/welcome"
    ],
    "pre-update-cmd": "@pre-craft-update",
    "pre-install-cmd": "@pre-craft-update",
    "post-update-cmd": "@post-craft-update",
    "post-install-cmd": "@post-craft-update"
  }
}
